{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": [
        "react",
        "html"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "linebreak-style": 0,
        //[ 0, "error", "windows" ]
        "no-underscore-dangle": "off",
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}